#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/capstone-ontology#conductsEvaluation
:conductsEvaluation rdf:type owl:ObjectProperty ;
                    rdfs:domain :ExternalEvaluator ;
                    rdfs:range :Evaluation .


###  http://www.semanticweb.org/capstone-ontology#evaluatedBy
:evaluatedBy rdf:type owl:ObjectProperty ;
             rdfs:domain :Project ;
             rdfs:range :Stakeholders ;
             rdfs:comment "Identifies who assesses the project outcomes" .


###  http://www.semanticweb.org/capstone-ontology#evaluatesDeliverable
:evaluatesDeliverable rdf:type owl:ObjectProperty ;
                      rdfs:domain :Evaluation ;
                      rdfs:range :Deliverables .


###  http://www.semanticweb.org/capstone-ontology#hasAdvisor
:hasAdvisor rdf:type owl:ObjectProperty ;
            rdfs:domain :Project ;
            rdfs:range [ owl:intersectionOf ( :Faculty
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :holdsRole ;
                                                owl:someValuesFrom :Advisor
                                              ]
                                            ) ;
                         rdf:type owl:Class
                       ] .


###  http://www.semanticweb.org/capstone-ontology#hasDeliverable
:hasDeliverable rdf:type owl:ObjectProperty ;
                rdfs:domain :Project ;
                rdfs:range :Deliverables ;
                rdfs:comment "Links project to its required outputs" .


###  http://www.semanticweb.org/capstone-ontology#hasPhase
:hasPhase rdf:type owl:ObjectProperty ;
          rdfs:domain :Project ;
          rdfs:range :Phases .


###  http://www.semanticweb.org/capstone-ontology#hasSupervisor
:hasSupervisor rdf:type owl:ObjectProperty ;
               rdfs:domain :Project ;
               rdfs:range [ owl:intersectionOf ( :Faculty
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :holdsRole ;
                                                   owl:someValuesFrom :Supervisor
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] .


###  http://www.semanticweb.org/capstone-ontology#hasTeamMember
:hasTeamMember rdf:type owl:ObjectProperty ;
               rdfs:domain :Project ;
               rdfs:range :Student ;
               rdfs:comment "Links a project to its student participants" .


###  http://www.semanticweb.org/capstone-ontology#holdsRole
:holdsRole rdf:type owl:ObjectProperty ;
           rdfs:domain :Stakeholders ;
           rdfs:range :ProjectRole ;
           rdfs:comment "Indicates which formal function a stakeholder performs" .


###  http://www.semanticweb.org/capstone-ontology#precedes
:precedes rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://www.w3.org/2006/time#intervalBefore> ;
          rdf:type owl:TransitiveProperty ;
          rdfs:domain :Phases ;
          rdfs:range :Phases ;
          rdfs:comment "Temporal ordering of project phases" .


###  http://www.semanticweb.org/capstone-ontology#producedBy
:producedBy rdf:type owl:ObjectProperty ;
            rdfs:domain :Deliverables ;
            rdfs:range :Student .


