#################################################################

###  http://www.semanticweb.org/capstone-ontology#hasAbstract
:hasAbstract rdf:type owl:DatatypeProperty ;
             rdfs:domain :Report ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/capstone-ontology#hasApprovalStatus
:hasApprovalStatus rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Deliverables .


###  http://www.semanticweb.org/capstone-ontology#hasDeadline
:hasDeadline rdf:type owl:DatatypeProperty ;
             rdfs:domain :Phases ;
             rdfs:range xsd:dateTime ;
             rdfs:comment "When the phase must be completed" .


###  http://www.semanticweb.org/capstone-ontology#hasDepartment
:hasDepartment rdf:type owl:DatatypeProperty ;
               rdfs:domain :Project ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/capstone-ontology#hasDueDate
:hasDueDate rdf:type owl:DatatypeProperty ;
            rdfs:domain :Phases ;
            rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/capstone-ontology#hasDuration
:hasDuration rdf:type owl:DatatypeProperty ;
             rdfs:domain :Phases ;
             rdfs:range xsd:duration ;
             rdfs:comment "The length of time a phase takes (e.g., P30D for 30 days)\"" .


###  http://www.semanticweb.org/capstone-ontology#hasEndDate
:hasEndDate rdf:type owl:DatatypeProperty ;
            rdfs:domain :Phases ;
            rdfs:range xsd:dateTime ;
            rdfs:comment "When the phase actually ended" .


###  http://www.semanticweb.org/capstone-ontology#hasFeedback
:hasFeedback rdf:type owl:DatatypeProperty ;
             rdfs:domain :Evaluation ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/capstone-ontology#hasGrade
:hasGrade rdf:type owl:DatatypeProperty ;
          rdfs:domain :Evaluation ;
          rdfs:range xsd:string .


###  http://www.semanticweb.org/capstone-ontology#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:domain :Faculty ;
         rdfs:range xsd:string .


###  http://www.semanticweb.org/capstone-ontology#hasPresentationDate
:hasPresentationDate rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Presentation ;
                     rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/capstone-ontology#hasScore
:hasScore rdf:type owl:DatatypeProperty ;
          rdfs:domain :Evaluation ;
          rdfs:range xsd:integer ;
          rdfs:comment "Numerical evaluation result (0-100 scale)" .


###  http://www.semanticweb.org/capstone-ontology#hasSlideCount
:hasSlideCount rdf:type owl:DatatypeProperty ;
               rdfs:domain :Presentation ;
               rdfs:range xsd:integer .


###  http://www.semanticweb.org/capstone-ontology#hasStartDate
:hasStartDate rdf:type owl:DatatypeProperty ;
